
public class Main {

	public static void main(String[] args) 
	{
		System.out.println("i++");
		int i = 0;
		for( i = 0; i < 10; i++)
		{
			System.out.println(i);
		}
		System.out.println("");
		
		System.out.println("++i");
		i = 0;
		for( i = 0; i < 10; ++i)
		{
			System.out.println(i);
		}
		System.out.println("");

	}

}
